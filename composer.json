{
    "name": "matthiasnoback/doctrine-orm-value-object-bundle",
    "type": "symfony-bundle",
    "description": "Bundle for integrating the doctrine-orm-value-object library in a Symfony2 project",
    "keywords": ["Doctrine ORM","value object"],
    "homepage": "http://github.com/matthiasnoback/doctrine-orm-value-object-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Matthias Noback",
            "email": "matthiasnoback@gmail.com",
            "homepage": "http://php-and-symfony.matthiasnoback.nl"
        }
    ],
    "require": {
        "symfony/framework-bundle": "2.*",
        "matthiasnoback/doctrine-orm-value-object": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "matthiasnoback/symfony-dependency-injection-test": "*",
        "matthiasnoback/symfony-service-definition-validator": "*"
    },
    "target-dir": "Noback/Bundle/DoctrineOrmValueObjectBundle",
    "autoload": {
        "psr-0" : { "Noback\\Bundle\\DoctrineOrmValueObjectBundle" : "" }
    }
}
